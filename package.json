{
  "name": "instantsearch-app",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://aleal4.github.io/algolia",
  "scripts": {
    "clean": "rm -rf dist/",
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url https://aleal4.github.io/algolia/",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "targets": {
    "default": {
      "publicUrl": "/algolia"
    }
  },
  "dependencies": {
    "algoliasearch": "4",
    "dotenv": "^16.4.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-instantsearch": "7.13.6"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "gh-pages": "^6.2.0",
    "parcel": "2.5.0",
    "process": "^0.11.10",
    "typescript": "4.7.4"
  }
}
